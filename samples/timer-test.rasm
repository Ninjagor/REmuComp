_START
  STRS R15, "It has been:", 0xE050
  
  MOVI R10, 0x0000
  MOVI R9, 0x0001
  MOVR R6, R9

  CALL _SET_TIMER

  JMP _LOOP

_SET_TIMER
  STDI 0x050
  CMP R10, R9
  CEQ _PRINT
  MOVR R10, R9
  
  RET

_PRINT
  PRINT R15, 1
  PRINT R6, 0
  ADD R6, R6, R9
  RET

_LOOP
  RTD R1 
  MOVI R2, 0x0000
  CMP R1, R2
  CEQ _SET_TIMER
  JMP _LOOP

